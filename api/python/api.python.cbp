			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="pt_ss_k_wrap.cxx">
			<Option target="pt_ss_k_Debug" />
			<Option target="pt_ss_k_Release" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<lib_finder disable_auto="1" />
			<debugger />
			<DoxyBlocks>
