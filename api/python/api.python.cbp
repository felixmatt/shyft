<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="api.python" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="api_Debug">
				<Option output="../../shyft/api/___init__" prefix_auto="0" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add directory="../../bin/Debug" />
				</Linker>
			</Target>
			<Target title="pt_gs_k_Debug">
				<Option output="../../shyft/api/_pt_gs_k" prefix_auto="0" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add directory="../../bin/Debug" />
				</Linker>
			</Target>
			<Target title="api_Release">
				<Option output="../../shyft/api/___init__" prefix_auto="0" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DARMA_NO_DEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add directory="../../bin/Release" />
				</Linker>
			</Target>
			<Target title="pt_gs_k_Release">
				<Option output="../../shyft/api/_pt_gs_k" prefix_auto="0" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DARMA_NO_DEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add directory="../../bin/Release" />
				</Linker>
			</Target>
			<Target title="pt_ss_k_Debug">
				<Option output="../../shyft/api/_pt_ss_k" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="0" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add directory="../../bin/Debug" />
				</Linker>
			</Target>
			<Target title="pt_ss_k_Release">
				<Option output="../../shyft/api/_pt_ss_k" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="0" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Linker>
					<Add directory="../../bin/Release" />
				</Linker>
			</Target>
			<Environment>
				<Variable name="numpy_include" value="/opt/anaconda/lib/python2.7/site-packages/numpy/core/include" />
				<Variable name="py_include" value="/opt/anaconda/include/python2.7" />
				<Variable name="py_lib" value="/opt/anaconda/lib" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="Debug" targets="api_Debug;pt_gs_k_Debug;pt_ss_k_Debug;" />
			<Add alias="Release" targets="api_Release;pt_gs_k_Release;pt_ss_k_Release;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-std=c++1y" />
			<Add option="-Wall" />
			<Add option="-fexceptions -fPIC -pthread" />
			<Add option="-DARMA_DONT_USE_WRAPPER" />
			<Add directory="$(py_include)" />
			<Add directory="$(numpy_include)" />
			<Add directory=".." />
			<Add directory="../../../dlib" />
			<Add directory="../.." />
		</Compiler>
		<Linker>
			<Add option="-pthread" />
			<Add library="shyftcore" />
			<Add library="blas" />
			<Add library="lapack" />
			<Add directory="$(py_lib)" />
			<Add directory="/usr/local/lib64" />
		</Linker>
		<ExtraCommands>
			<Add before="make -f Makefile.swig_run -j 8" />
		</ExtraCommands>
		<Unit filename="Makefile.swig_run" />
		<Unit filename="__init__.i">
			<Option compile="1" />
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="__init___wrap.cxx">
